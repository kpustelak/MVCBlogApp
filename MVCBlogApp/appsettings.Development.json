{
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=blog;Username=blog;Password=blog"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/BlogLogs.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}